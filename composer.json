{
  "name": "devhunters/devhunters-fw",
  "description": "A Framework to Make Creating WordPress Plugins Easier",
  "license": "MIT",
  "version": "1.0.0",
  "author": {
    "name": "DevHunters",
    "homepage": "https://devhunters.dev"
  },
  "support": {
    "email": "devhunterssupp@gmail.com",
    "url": "https://github.com/puyaalexxx/devhunters-fw",
    "issues": "https://github.com/puyaalexxx/devhunters-fw/issues",
    "docs": "https://github.com/puyaalexxx/devhunters-fw"
  },
  "extra": {
    "name": "DevHunters Framework",
    "wordpress-version": "6.5"
  },
  "type": "wordpress-plugin",
  "require": {
    "php": ">=8.0",
    "phenx/php-font-lib": "^1.0",
    "josegonzalez/dotenv": "^4.0"
  },
  "autoload": {
    "files": [
      "constants.php",
      "helpers/fw.php",
      "helpers/general.php",
      "helpers/enqueue.php",
      "helpers/posts.php",
      "helpers/terms.php",
      "helpers/options.php",
      "helpers/validations.php",
      "helpers/vb.php",
      "helpers/features.php"
    ],
    "psr-4": {
      "DHT\\": "",
      "DHT\\Helpers\\": "helpers/",
      "DHT\\Helpers\\Classes\\": "helpers/classes/",
      "DHT\\Helpers\\Traits\\": "helpers/traits/",
      "DHT\\Helpers\\Traits\\Options\\": "helpers/traits/options/",
      "DHT\\Helpers\\Traits\\Singletons\\": "helpers/traits/singletons/",
      "DHT\\Config\\": "config/",
      "DHT\\Core\\": "core/",
      "DHT\\Core\\API\\": "core/api/",
      "DHT\\Core\\API\\API\\": "core/api/api/",
      "DHT\\Core\\CLI\\": "core/cli/",
      "DHT\\Core\\Options\\": "core/options/",
      "DHT\\Core\\Options\\Containers\\": "core/options/containers/",
      "DHT\\Core\\Options\\Containers\\Containers\\": "core/options/containers/containers/",
      "DHT\\Core\\Options\\Groups\\": "core/options/groups/",
      "DHT\\Core\\Options\\Groups\\Groups\\": "core/options/groups/groups/",
      "DHT\\Core\\Options\\Toggles\\": "core/options/toggles/",
      "DHT\\Core\\Options\\Toggles\\Toggles\\": "core/options/toggles/toggles/",
      "DHT\\Core\\Options\\Fields\\": "core/options/fields/",
      "DHT\\Core\\Options\\Fields\\Fields\\": "core/options/fields/fields/",
      "DHT\\Extensions\\": "extensions/",
      "DHT\\Extensions\\CPT\\": "extensions/cpt/",
      "DHT\\Extensions\\DashPages\\": "extensions/dashboard-pages/",
      "DHT\\Extensions\\Sidebars\\": "extensions/sidebars/",
      "DHT\\Extensions\\VB\\": "extensions/vb/",
      "DHT\\Extensions\\VB\\Components\\": "extensions/vb/components/"
    }
  },
  "require-dev": {
    "php-stubs/wordpress-stubs": "^6.6",
    "php-stubs/wp-cli-stubs": "^2.10"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
