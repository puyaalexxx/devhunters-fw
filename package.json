{
  "name": "devhunters-fw",
  "version": "1.0.0",
  "description": "**The framework used in my plugins**",
  "main": "index.js",
  "scripts": {
    "build:prod:vite": "vite build --mode production && node helpers/node/remove-js-generated-files.js",
    "build:dev:vite": "tsc && vite build --mode development",
    "build:watch:vite": "tsc && vite build --watch --mode development",
    "test": "vitest",
    "start": "vite",
    "check-types": "tsc --noEmit",
    "lint": "eslint --fix",
    "format": "prettier --write",
    "clean:prod": "rimraf assets/scripts/modules/*.js"
  },
  "keywords": [],
  "author": "DevHunters",
  "license": "ISC",
  "devDependencies": {
    "@types/ace": "^0.0.52",
    "@types/jquery": "^3.5.30",
    "@types/jquery.timepicker": "^0.3.34",
    "@types/jquery.ui.datetimepicker": "^0.3.35",
    "@types/jqueryui": "^1.12.23",
    "@types/select2": "^4.0.63",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "glob": "^11.0.0",
    "postcss-assets": "^6.0.0",
    "postcss-cli": "^11.0.0",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-mixins": "^11.0.1",
    "postcss-nested": "^6.2.0",
    "postcss-preset-env": "^10.0.3",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "3.3.3",
    "rollup": "^4.22.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.6"
  },
  "dependencies": {
    "ace-builds": "^1.36.2"
  }
}
